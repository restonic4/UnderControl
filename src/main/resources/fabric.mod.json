{
  "schemaVersion": 1,
  "id": "under_control",
  "version": "${version}",
  "name": "UnderControl",
  "description": "Library to make things easier",
  "authors": [
    "restonic4"
  ],
  "contact": {
    "repo": "https://github.com/restonic4/UnderControl"
  },
  "license": "MIT",
  "icon": "assets/under_control/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.chaotic_loom.under_control.client.UnderControlClient"
    ],
    "main": [
      "com.chaotic_loom.under_control.UnderControl"
    ],
    "server": [
      "com.chaotic_loom.under_control.UnderControlServer"
    ],
    "modmenu": [
      "com.chaotic_loom.under_control.compatibility.modmenu.ModMenuAPIImpl"
    ]
  },
  "mixins": [
    "under_control.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "accessWidener": "mod.accesswidener",
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.modrinth": "https://modrinth.com/organization/chaotic-loom",
        "modmenu.curseforge": "https://www.curseforge.com/minecraft/mc-mods/under-control"
      },
      "badges": [
        "special_library",
        "chaotic_loom"
      ]
    }
  }
}
